# Backend Configuration Example
# Copy this file to backend.tf and update with your S3 bucket details

terraform {
  backend "s3" {
    bucket         = "your-terraform-state-bucket"
    key            = "gameday-ddos/terraform.tfstate"
    region         = "us-east-1"
    encrypt        = true
    dynamodb_table = "terraform-state-lock"
  }
}